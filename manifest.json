{
  "manifest_version": 2,
  "name": "FilePass",
  "version": "1.0.0",
  "description": "Use files instead of passwords",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "sjcl.min.js",
        "filepass.js",
        "index.js"
      ],
      "css": [
        "filepass.css"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  }
}